<h1>Collaboration</h1>

<div class="row">
  <div class="col-md-11">
    <h4><strong><p>Add collaborators to the wiki:</p></strong></h4>
    <%= form_for [@collaboration.wiki, @collaboration] do |f| %>
      <div class="form-group">
        <%= f.label :email %>
        <%= f.select :user_id options_from_collection_for_select(@users, "id", "email") %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
        <%= link_to "Cancel", wikis_path, class: 'btn btn-info' %>
      </div>
    <% end %>
  </div>
</div>
